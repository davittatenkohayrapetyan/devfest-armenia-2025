{
  "name": "devfest-armenia-2025",
  "version": "1.0.0",
  "description": "Single-page PWA for DevFest Armenia 2025 (Dec 20, Woods Center)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate:data": "node scripts/generate-data.mjs",
    "generate:share-pages": "node scripts/generate-share-pages.mjs",
    "generate:icons": "node scripts/generate-icons.mjs",
    "prebuild": "npm run generate:icons && npm run generate:data && npm run generate:share-pages",
    "predev": "npm run generate:data"
  },
  "keywords": [
    "devfest",
    "armenia",
    "gdg",
    "pwa"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.5",
    "autoprefixer": "^10.4.17",
    "exceljs": "^4.4.0",
    "postcss": "^8.4.33",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^7.1.11",
    "vite-plugin-pwa": "^1.1.0"
  },
  "dependencies": {
    "workbox-window": "^7.0.0"
  }
}
